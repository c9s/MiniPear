        <mirror host="de.pear.php.net" ssl="yes">
            <rest>
                <baseurl type="REST1.0">https://de.pear.php.net/rest/</baseurl>
                <baseurl type="REST1.1">https://de.pear.php.net/rest/</baseurl>
